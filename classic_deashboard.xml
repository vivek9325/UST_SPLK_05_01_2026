<form version="1.1" theme="dark">
  <label>vk_classic_dashboard</label>
  
  
  <search id="base_search1">
    
    
    <query>
      
      index=main source=Sample_tickets.csv | stats count by severity, current_ticket_state, asset_id, application_name
      
    </query>
    
    <earliest>0</earliest>
    <latest>now</latest>
    
  </search>
  
  
  <search id="saved_search" ref="vk_savedsearch"></search>
  
  
  
  <fieldset submitButton="true" autoRun="false">
    <input type="time" token="time_field" searchWhenChanged="false">
      <label>Time</label>
      <default>
        <earliest>-30d@d</earliest>
        <latest>now</latest>
      </default>
    </input>
    
    
    <input type="dropdown" token="sev" searchWhenChanged="false">
      <label>severity</label>
      <fieldForLabel>severity</fieldForLabel>
      <fieldForValue>severity</fieldForValue>
     
     
      <search base="base_search1">
        <query> | stats count by severity</query>
      </search>
     
     
      <choice value="*">All</choice>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
    
    
    
    <input type="multiselect" token="state" searchWhenChanged="false">
      <label>Ticket State</label>
      <valuePrefix>current_ticket_state="</valuePrefix>
      <valueSuffix>"</valueSuffix>
      <delimiter> OR </delimiter>
      <fieldForLabel>current_ticket_state</fieldForLabel>
      <fieldForValue>current_ticket_state</fieldForValue>
     
     
      <search base="saved_search">
        <query>| stats count by current_ticket_state</query>
      </search>
    
    
    </input>
    <input type="radio" token="app_name" searchWhenChanged="false">
      <label>Application name</label>
      <default>Admin</default>
      <initialValue>Admin</initialValue>
      <fieldForLabel>application_name</fieldForLabel>
      <fieldForValue>application_name</fieldForValue>
      <search>
        <query>index=main source=Sample_tickets.csv | stats count by application_name</query>
        <earliest>0</earliest>
        <latest></latest>
      </search>
    </input>
    <input type="checkbox" token="asset_id" searchWhenChanged="false">
      <label>asset_id</label>
      <valuePrefix>asset_id="</valuePrefix>
      <valueSuffix>"</valueSuffix>
      <delimiter> OR </delimiter>
      <fieldForLabel>asset_id</fieldForLabel>
      <fieldForValue>asset_id</fieldForValue>
      <search>
        <query>index=main source=Sample_tickets.csv | stats count by asset_id</query>
        <earliest>0</earliest>
        <latest></latest>
      </search>
    </input>
  </fieldset>
  <row>
    <panel>
      <title>count by severity</title>
      <chart>
        <search>
          <query>index=main source=Sample_tickets.csv $state$| stats count by severity</query>
          <earliest>$time_field.earliest$</earliest>
          <latest>$time_field.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
        <option name="charting.axisTitleX.visibility">visible</option>
        <option name="charting.axisTitleY.visibility">visible</option>
        <option name="charting.axisTitleY2.visibility">visible</option>
        <option name="charting.axisX.abbreviation">none</option>
        <option name="charting.axisX.scale">linear</option>
        <option name="charting.axisY.abbreviation">none</option>
        <option name="charting.axisY.scale">linear</option>
        <option name="charting.axisY2.abbreviation">none</option>
        <option name="charting.axisY2.enabled">0</option>
        <option name="charting.axisY2.scale">inherit</option>
        <option name="charting.chart">column</option>
        <option name="charting.chart.bubbleMaximumSize">50</option>
        <option name="charting.chart.bubbleMinimumSize">10</option>
        <option name="charting.chart.bubbleSizeBy">area</option>
        <option name="charting.chart.nullValueMode">gaps</option>
        <option name="charting.chart.showDataLabels">none</option>
        <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
        <option name="charting.chart.stackMode">default</option>
        <option name="charting.chart.style">shiny</option>
        <option name="charting.drilldown">all</option>
        <option name="charting.layout.splitSeries">0</option>
        <option name="charting.layout.splitSeries.allowIndependentYRanges">0</option>
        <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
        <option name="charting.legend.mode">standard</option>
        <option name="charting.legend.placement">right</option>
        <option name="charting.lineWidth">2</option>
        <option name="refresh.display">preview</option>
        <option name="trellis.enabled">0</option>
        <option name="trellis.scales.shared">1</option>
        <option name="trellis.size">medium</option>
        <drilldown>
          <set token="tok1">$click.value$</set>
        </drilldown>
      </chart>
    </panel>
    <panel>
      <title>count by ticket state - $tok1$</title>
      <chart>
        <search>
          <query>index=main source=Sample_tickets.csv severity=$sev$ application_name=$app_name$ $asset_id$| stats count by current_ticket_state</query>
          <earliest>0</earliest>
          <latest></latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
        <option name="charting.axisTitleX.visibility">visible</option>
        <option name="charting.axisTitleY.visibility">visible</option>
        <option name="charting.axisTitleY2.visibility">visible</option>
        <option name="charting.axisX.abbreviation">none</option>
        <option name="charting.axisX.scale">linear</option>
        <option name="charting.axisY.abbreviation">none</option>
        <option name="charting.axisY.scale">linear</option>
        <option name="charting.axisY2.abbreviation">none</option>
        <option name="charting.axisY2.enabled">0</option>
        <option name="charting.axisY2.scale">inherit</option>
        <option name="charting.chart">column</option>
        <option name="charting.chart.bubbleMaximumSize">50</option>
        <option name="charting.chart.bubbleMinimumSize">10</option>
        <option name="charting.chart.bubbleSizeBy">area</option>
        <option name="charting.chart.nullValueMode">gaps</option>
        <option name="charting.chart.showDataLabels">none</option>
        <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
        <option name="charting.chart.stackMode">default</option>
        <option name="charting.chart.style">shiny</option>
        <option name="charting.drilldown">none</option>
        <option name="charting.layout.splitSeries">0</option>
        <option name="charting.layout.splitSeries.allowIndependentYRanges">0</option>
        <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
        <option name="charting.legend.mode">standard</option>
        <option name="charting.legend.placement">right</option>
        <option name="charting.lineWidth">2</option>
        <option name="refresh.display">progressbar</option>
        <option name="trellis.enabled">0</option>
        <option name="trellis.scales.shared">1</option>
        <option name="trellis.size">medium</option>
      </chart>
    </panel>
  </row>
</form>
